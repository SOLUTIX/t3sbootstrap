							<and>
								<numIndex index="0">USER:T3SBS\T3sbootstrap\UserFunction\TcaMatcher->cardWrapperParent</numIndex>
								<numIndex index="1">FIELD:sDEF.multiImage.enable:=:0</numIndex>
								<numIndex index="2">FIELD:sDEF.tiling.enable:=:0</numIndex>
							</and>
						</displayCond>
						<config>
							<type>check</type>
