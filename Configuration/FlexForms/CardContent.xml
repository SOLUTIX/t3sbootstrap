<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<sheetTitle>Content</sheetTitle>
				<type>array</type>
				<el>
					<header.text>
						<label>Card Header</label>
						<config>
							<type>input</type>
							<size>48</size>
							<max>99</max>
							<eval>trim</eval>
							<checkbox>0</checkbox>
						</config>
					</header.text>
					<text.top>
						<label>Text Top</label>
						<config>
							 <type>text</type>
							 <cols>int,trim</cols>
							 <rows>3</rows>
							 <enableRichtext>true</enableRichtext>
						</config>
					</text.top>
					<list.container>
						<section>1</section>
						<type>array</type>
						<el>
							<list>
								<title>List-Item</title>
								<type>array</type>
								<el>
									<group>
										<label>List Group</label>
										<config>
											<type>input</type>
											<size>48</size>
										</config>
									</group>
								</el>
							</list>
						</el>
					</list.container>
					<text.bottom>
						<label>Text Bottom (optional: new .card-block)</label>
						<config>
							 <type>text</type>
							 <cols>int,trim</cols>
							 <rows>3</rows>
							 <enableRichtext>true</enableRichtext>
						</config>
					</text.bottom>
					<footer.text>
						<label>Card Footer</label>
						<config>
							<type>input</type>
							<size>48</size>
							<max>99</max>
							<eval>trim</eval>
							<checkbox>0</checkbox>
						</config>
					</footer.text>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
