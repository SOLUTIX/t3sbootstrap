<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<sheetTitle>LLL:EXT:t3sbootstrap/Resources/Private/Language/locallang_be.xlf:flexforms.general</sheetTitle>
				<type>array</type>
				<el>
					<hidden>
						<label>Hiding / Display Elements</label>
						<config>
							<type>select</type>
							<renderType>selectSingle</renderType>
							<items>
								<numIndex index="0">
									<label>none</label>
									<value>0</value>
								</numIndex>
								<numIndex index="1">
									<label>Hidden only on xs</label>
									<value>d-none d-sm-block</value>
								</numIndex>
								<numIndex index="2">
									<label>Hidden on sm and down</label>
									<value>d-none d-md-block</value>
								</numIndex>
								<numIndex index="3">
									<label>Hidden on md and down</label>
									<value>d-none d-lg-block</value>
								</numIndex>
								<numIndex index="4">
									<label>Hidden on lg and down</label>
									<value>d-none d-xl-block</value>
								</numIndex>
								<numIndex index="5">
									<label>Hidden only on xl</label>
									<value>d-xl-none</value>
								</numIndex>
								<numIndex index="6">
									<label>Visible only on xs</label>
									<value>d-block d-sm-none</value>
								</numIndex>
								<numIndex index="7">
									<label>Visible sm and down</label>
									<value>d-block d-md-none</value>
								</numIndex>
								<numIndex index="8">
									<label>Visible md and down</label>
									<value>d-block d-lg-none</value>
								</numIndex>
								<numIndex index="9">
									<label>Visible lg and down</label>
									<value>d-block d-xl-none</value>
								</numIndex>
							</items>
							<default>0</default>
						</config>
					</hidden>

					<width>
						<label>max. Image width</label>
						<config>
							<type>number</type>
							<size>10</size>
							<max>4</max>
							<default>576</default>
						</config>
					</width>

					<speedFactor>
						<label>Parallax effect speed. Provide numbers from -1.0 to 2.0</label>
						<config>
							<type>input</type>
							<size>10</size>
							<default>1</default>
						</config>
					</speedFactor>

					<addHeight>
						<label>Surcharge height to parallax view (in px)</label>
						<config>
							<type>number</type>
							<size>10</size>
							<default>100</default>
						</config>
					</addHeight>

					<mobile>
						<label>Disable on mobile devices</label>
						<config>
							<type>check</type>
							<default>1</default>
						</config>
					</mobile>

				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
